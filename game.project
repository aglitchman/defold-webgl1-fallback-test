app_icon_144x144 = /builtins/assets/images/logo/logo_256.png
app_icon_192x192 = /builtins/assets/images/logo/logo_256.png
package = com.webgl1.fallback.test

[project]
title = webgl1_fallback_test
version = 1.0.0

[html5]
heap_size = 128
cssfile = /builtins/manifests/web/dark_theme.css
scale_mode = fit

[render]
